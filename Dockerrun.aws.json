{
  "AWSEBDockerrunVersion": 2,
  "containerDefinitions": [
    {
      "name": "landingpage",
      "image": "alexlee27/fm8landingpage",
      "hostname": "client",
      "essential": true,
      "portMappings": [
        {
          "hostPort": 80,
          "containerPort": 80
        }
      ],
      "memory": 128
    },
    {
      "name": "prometheus",
      "image": "alexlee27/test-prometheus",
      "hostname": "prometheus",
      "essential": false,
      "portMappings": [
        {
          "hostPort": 9090,
          "containerPort": 9090
        }
      ]
    }
  ]
}